{% extends ':App:layout.html.twig' %}

{% block body %}
    <h1>Interviewuser</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ interviewUser.id }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{% if interviewUser.status %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('interviewuser_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('interviewuser_edit', { 'id': interviewUser.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
